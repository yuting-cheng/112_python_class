   "source": [
    "from apscheduler.schedulers.blocking import BlockingScheduler\n",
    "\n",
    "def my_task():\n",
    "    print(\"執行我的任務\")\n",
    "\n",
    "# 創建調度器\n",
    "scheduler = BlockingScheduler()\n",
    "\n",
    "# 添加一個每5秒執行一次的任務\n",
    "scheduler.add_job(my_task, 'interval', seconds=5)\n",
    "\n",
    "# 開始運行調度器\n",
    "scheduler.start()\n"
